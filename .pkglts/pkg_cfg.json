{
  "_pkglts": {
    "auto_install": true,
    "install_front_end": "stdout",
    "use_prompts": false,
    "version": 6
  },
  "base": {
    "authors": [
      [
        "revesansparole",
        "revesansparole@gmail.com"
      ]
    ],
    "namespace": null,
    "namespace_method": "pkg_util",
    "pkgname": "pkglts",
    "url": null
  },
  "conda": {},
  "coverage": {},
  "coveralls": {},
  "data": {},
  "doc": {
    "description": "Building packages with long term support",
    "keywords": [
      "packaging",
      "package builder"
    ]
  },
  "flake8": {},
  "github": {
    "owner": "{{ base.authors[0][0] }}",
    "project": "{{ base.pkgname }}",
    "url": "https://github.com/{{ github.owner }}/{{ github.project }}"
  },
  "landscape": {},
  "license": {
    "name": "CeCILL-C",
    "organization": "OpenAlea",
    "project": "{{ base.pkgname }}",
    "year": "2015"
  },
  "notebook": {
    "src_directory": "example"
  },
  "pypi": {
    "classifiers": [
      "Development Status :: 2 - Pre-Alpha",
      "Intended Audience :: Developers",
      "License :: OSI Approved :: BSD License",
      "Natural Language :: English"
    ]
  },
  "pysetup": {
    "intended_versions": [
      "27",
      "35",
      "36"
    ],
    "require": [
      {
        "name": "jinja2"
      }
    ]
  },
  "readthedocs": {
    "project": "{{ github.project }}"
  },
  "sphinx": {
    "autodoc_dvlpt": true,
    "theme": "default"
  },
  "test": {
    "suite_name": "pytest"
  },
  "tox": {},
  "travis": {},
  "version": {
    "major": 2,
    "minor": 0,
    "post": 0
  }
}